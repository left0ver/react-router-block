{
  "version": "0.1.3",
  "license": "MIT",
  "description": "基于react-router v6版本的路由守卫，让你能使用类似vue-router那样的路由守卫",
  "keywords": [
    "react-router-block",
    "react路由守卫",
    "react-router-guard",
    "react-router-waiter",
    "路由守卫"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/left0ver/react-router-block.git"
  },
  "bugs": {
    "url": "https://github.com/left0ver/react-router-block/issues"
  },
  "homepage": "https://github.com/left0ver/react-router-block#readme",
  "scripts": {
    "patch": "npm version patch && npm publish",
    "minor": "npm version minor && npm publish",
    "major": "npm version major && npm publish",
    "tag": "git tag -a v%npm_package_version% && git push origin v%npm_package_version%",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-router-dom": "^6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-router-block",
  "author": "left0ver",
  "module": "dist/react-router-block.esm.js",
  "size-limit": [
    {
      "path": "dist/react-router-block.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-router-block.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "husky": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
